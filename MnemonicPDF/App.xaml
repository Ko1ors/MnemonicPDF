<Application x:Class="MnemonicPdf.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MnemonicPdf"
             xmlns:converters="clr-namespace:MnemonicPdf.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="WindowsBrush" Color="#383e56" />
            <SolidColorBrush x:Key="BackgroundBrush" Color="#F3F0F4" />
        <SolidColorBrush x:Key="TextBrush" Color="#F8E7A2" />
        <converters:SizeConverter x:Key="sizeConverter"/>
    </Application.Resources>
</Application>
